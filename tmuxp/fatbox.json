{
  "session_name": "default",
  "windows": [
    {
      "layout": "main-vertical",
      "panes": [
        {
          "focus": "true",
          "shell_command": "btop"
        },
        "docker stats",
        "watch nvidia-smi"
      ],
      "start_directory": "/home/box",
      "window_name": "monitor"
    },
    {
      "layout": "tiled",
      "panes": [
        "dl nginx",
        {
          "focus": "true",
          "shell_command": "cd ~/src/fatbox/docker && git status"
        },
        "cd ~/src/fatbox/docker && nvim docker-compose.yml",
        "cd ~/src/fatbox/docker"
      ],
      "start_directory": "/home/box",
      "window_name": "docker"
    },
    {
      "focus": "true",
      "panes": [
        "pane"
      ],
      "start_directory": "/home/box",
      "window_name": "work"
    }
  ]
}
